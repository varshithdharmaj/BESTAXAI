{
  "name": "bettertaxai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "homepage": "https://varshithdharmaj.github.io/BETTERTAXAI",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "deploy:github": "cross-env GITHUB_PAGES=true npm run build && gh-pages -d dist",
    "build:docs": "cross-env DOCS_DEPLOY=true npm run build && Copy-Item -Recurse -Force dist\\* docs\\",
    "docker:build": "docker build -t bettertaxai .",
    "docker:run": "docker run -p 3000:3000 bettertaxai",
    "docker:dev": "docker-compose --profile dev up bettertaxai-dev",
    "docker:prod": "docker-compose up bettertaxai"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.0.0",
    "@radix-ui/react-avatar": "^1.0.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-dropdown-menu": "^2.0.0",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tabs": "^1.0.0",
    "@radix-ui/react-toast": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@tanstack/react-query": "^5.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.400.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.0.0",
    "tailwind-merge": "^2.0.0",
    "wouter": "^3.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "cross-env": "^10.0.0",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.3.0",
    "tailwindcss-animate": "^1.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.4.0"
  }
}
